max_threads=$(nproc)
echo "MAX THREADS :$max_threads"
echo "-------------------------"

cd ./kmeans_openmp
	for ((i = 1; i <= max_threads; i *= 2)); do
    	echo "------------ Current thread count: $i ------------------"
		./kmeans -n $i -i ../../../data/kmeans/inputGen/10000000_34f.txt
	done
cd ..

cd ./kmeans_serial
	./kmeans -i ../../../data/kmeans/inputGen/10000000_34f.txt
cd ..